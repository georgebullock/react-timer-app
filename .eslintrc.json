{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2019,
		"sourceType": "module"
	},
	"extends": ["eslint:recommended", "eslint-config-prettier"],
	"plugins": ["flowtype"],
	"rules": {
		"no-console": "off",
		"react/react-in-jsx-scope": "off",
		"no-unused-vars": "off"
	},
	"env": {
		"browser": true,
		"node": true,
		"jest": true
	}
}
